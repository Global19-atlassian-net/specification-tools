<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <parent>
        <groupId>org.eclipse.ee4j</groupId>
        <artifactId>project</artifactId>
        <version>1.0.5</version>
    </parent>

    <groupId>org.eclipse.ee4j.xml.soap-api</groupId>
    <artifactId>xml.soap-api-parent</artifactId>
    <packaging>pom</packaging>
    <version>1.5.0-SNAPSHOT</version>
    <name>${extension.name} API</name>
    <description>SAAJ API - Provides the API for creating and building SOAP messages.</description>
    <url>https://projects.eclipse.org/proposals/eclipse-project-jax-ws</url>

    <scm>
        <connection>scm:git:git://github.com/eclipse-ee4j/saaj-api.git</connection>
        <developerConnection>scm:git:git@github.com/eclipse-ee4j/saaj-api.git</developerConnection>
        <url>https://github.com/eclipse-ee4j/saaj-api</url>
        <tag>HEAD</tag>
    </scm>

    <properties>
        <extension.name>jakarta.xml.soap</extension.name>
        <spec.version>1.5</spec.version>
        <new.spec.version>1.5.0</new.spec.version>
        <non.final>false</non.final>
        <findbugs.exclude>exclude.xml</findbugs.exclude>
        <findbugs.threshold>Low</findbugs.threshold>
        <activation.version>1.2.1</activation.version>
        <legal.doc.source>${project.basedir}</legal.doc.source>
        <vendor.name>Oracle</vendor.name>
    </properties>

    <modules>
        <module>api</module>
    </modules>

    <licenses>
        <license>
            <name>Eclipse Distribution License - v 1.0</name>
            <url>http://www.eclipse.org/org/documents/edl-v10.php</url>
            <distribution>repo</distribution>
        </license>
    </licenses>

</project>
